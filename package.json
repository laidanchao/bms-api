    "requestretry": "^5.0.0",
    "require-glob": "^3.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "ts-node-dev": "^1.1.6",
    "type-cacheable": "^4.1.2",
    "typeorm": "0.2.41",
    "typeorm-naming-strategies": "1.1.0",
    "typeorm-transactional-cls-hooked": "0.1.12",
    "urlencode": "^1.1.0",
    "uuid": "^8.3.2",
