    "xlsx": "^0.16.9",
    "xlsx-style": "^0.8.13",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@ftlab/schematics": "0.0.2",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@playwright/test": "1.17.0",
    "@types/bull": "3.15.1",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/i18n": "^0.12.0",
    "@types/ioredis": "4.27.4",
    "@types/jest": "26.0.10",
    "@types/js-yaml": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/redis": "^2.8.28",
    "@types/request-promise": "^4.1.46",
    "@types/requestretry": "^1.12.7",
    "@types/supertest": "^2.0.8",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "auto-changelog": "^1.16.2",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-sonarjs": "^0.10.0",
    "git-branch-is": "^4.0.0",
    "husky": "^4.2.5",
    "jest": "26.4.2",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.2",
    "pg": "^8.5.1",
    "plop": "^2.7.4",
    "prettier": "^1.19.1",
    "sonar-scanner": "^3.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.2",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx,tsx,ts,js,json,css,md}": [
      "prettier --write"
    ]
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "engines": {
    "node": ">=18"
  }
}
