{
  "name": "bms-api",
  "version": "0.0.1",
  "description": "BMS API V2",
  "private": true,
  "scripts": {
    "start_web": "ts-node-dev --respawn -r tsconfig-paths/register src/app/web/web.app.ts",
    "start_web_cn": "ts-node-dev --respawn -r tsconfig-paths/register src/app/order/order.app.ts",
    "start_order": "ts-node-dev --respawn -r tsconfig-paths/register src/app/order/order.app.ts",
    "start_scheduler": "ts-node-dev --respawn -r tsconfig-paths/register src/app/scheduler/scheduler.app.ts",
    "start_crawler": "ts-node-dev --respawn -r tsconfig-paths/register src/app/crawler/crawler.app.ts",
    "start_scheduler_staging": "ts-node-dev --respawn -r tsconfig-paths/register src/app/stagingScheduler/scheduler.staging.app.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "scheduler": "ts-node -r tsconfig-paths/register src/app/scheduler/scheduler.app.ts",
    "scheduler_staging": "ts-node -r tsconfig-paths/register src/app/stagingScheduler/scheduler.staging.app.ts",
    "scheduler:server": "cd dist && node src/app/scheduler/scheduler.app.js",
    "scheduler_staging:server": "cd dist && node src/app/stagingScheduler/scheduler.staging.app.js",
    "crawler:server": "cd dist && node src/app/crawler/crawler.app.js",
    "start": "ts-node -r tsconfig-paths/register src/app/web/web.app.ts",
    "start:staging": "cross-env NODE_ENV=staging ts-node -r tsconfig-paths/register src/app/web/web.app.ts",
    "start:server": "cd dist && node src/app/web/web.app.js",
    "start:server:order": "cd dist && node src/app/order/order.app.js",
    "start:dev": "set NODE_ENV=development && nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/domain/base/repository/config/type-orm.config.ts",
    "migrate:create": "npm run typeorm migration:create -- -n",
    "migrate:generate": "npm run typeorm migration:generate -- -n",
    "migrate:revert": "npm run typeorm migration:revert",
    "migrate:run": "npm run typeorm migration:run",
    "migrate:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/test/seed/orm-seed.config.ts migration:run",
    "test": "jest --detectOpenHandles",
    "coverage": "jest --coverage --detectOpenHandles",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner -Dsonar.login=${sonarLogin}",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "plop": "plop"
  },
  "dependencies": {
    "@ftlab/nestjs": "0.1.22",
    "@ftlab/node-soap": "^1.1.0",
    "@ftlab/pdf-utils": "0.1.5",
    "@ftlab/xpush-client": "^1.0.9",
    "@nestjs/bull": "0.3.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/microservices": "^7.6.15",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.5.3",
    "@nestjs/typeorm": "7.0.0",
    "@nestjsx/crud": "4.6.2",
    "@nestjsx/crud-typeorm": "4.6.2",
    "@rob3000/nestjs-kafka": "1.6.0",
    "@sentry/node": "^5.27.6",
    "@softbrains/common-utils": "0.1.28",
    "@softbrains/feathers": "0.3.20",
    "@softbrains/ftp": "^1.1.4",
    "@type-cacheable/core": "^9.0.1",
    "@type-cacheable/redis-adapter": "^9.0.1",
    "adm-zip": "0.5.5",
    "async-lock": "^1.3.0",
    "atob": "^2.1.2",
    "aws-sdk": "^2.787.0",
    "aws4": "^1.12.0",
    "axios": "^0.21.1",
    "btoa": "^1.2.1",
    "bull": "3.22.9",
    "canvas": "^2.11.2",
    "cheerio": "^1.0.0-rc.10",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "compression": "latest",
    "cron-parser": "^4.7.1",
    "dd-trace": "^0.29.0",
    "decimal.js": "^10.3.1",
    "exceljs": "^4.2.0",
    "fs": "0.0.1-security",
    "helmet": "4.6.0",
    "https-proxy-agent": "^7.0.5",
