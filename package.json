    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.15",
    "jspdf-customfonts": "0.0.4-rc.4",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.20",
    "mathjs": "^9.5.0",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mustache": "^4.0.1",
    "nacos": "^2.6.0",
    "nest-raven": "^7.0.0",
    "nestjs-cls": "^1.6.2",
    "node-cache": "^5.1.2",
    "papaparse": "^5.3.0",
    "parquetjs": "^0.11.2",
    "passport-jwt": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "playwright": "1.17.0",
    "png-js": "^1.0.0",
